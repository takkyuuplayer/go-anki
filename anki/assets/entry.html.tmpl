<h2>{{.Headword}} ({{.FunctionalLabel}})</h2>

{{with .Pronunciation }}
<h4>Pronunciation ({{.Notation}})</h4>
  {{ range $index, $val := .Accents }}
    {{if $index}}/{{end}}
    {{with .AccentLabel }}
      {{ . }}
    {{end}}
    {{ .Spelling }}
  {{end}}
{{end}}

{{with .Inflections}}
<h4>Inflection</h4>
  {{ range $index, $val := . }}
    {{if $index}}/{{end}}
    {{with .FormLabel }}
      ({{ . }})
    {{end}}
    {{ .InflectedForm }}
    {{with .Pronunciation }}
      ({{ (index .Accents 0).Spelling }})
    {{end}}
  {{end}}
{{end}}

{{with .Definitions}}
<h4>Definition</h4>
<ol>
  {{ range $i, $v := . }}
  <li>{{.Sense}}</li>
  <ul>
    {{ range $key2, $vi := .Examples }}
    <li>{{.}}</li>
    {{end}}
  </ul>
  {{end}}
</ol>
{{end}}

